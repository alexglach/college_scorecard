<h1>Admissions</h1>

<table class="table table-striped">
  
  <tr>
    <td>Overall Admission Rate</td>
    <td><%= to_percent(school.admission.admission_rate_overall) %></td>
  </tr>

  <tr>
    <td>SAT - Average Overall</td>
    <td><%= float_to_int(school.admission.sat_scores_average_overall) %></td>
  </tr>

  <tr>
    <td>SAT - Critical Reading (25th - 75th Percentile)</td>
    <td><%= float_to_int(school.admission.sat_scores_25th_percentile_critical_reading) %> - 
    <%= float_to_int(school.admission.sat_scores_75th_percentile_critical_reading) %></td>
  </tr>

  <tr>
    <td>SAT - Math (25th - 75th Percentile)</td>
    <td><%= float_to_int(school.admission.sat_scores_25th_percentile_math) %> - 
    <%= float_to_int(school.admission.sat_scores_75th_percentile_math) %></td>
  </tr>

  <tr>
    <td>SAT - Writing (25th - 75th Percentile)</td>
    <td><%= float_to_int(school.admission.sat_scores_25th_percentile_writing) %> - 
    <%= float_to_int(school.admission.sat_scores_75th_percentile_writing) %></td>
  </tr>

  <tr>
    <td>ACT - Cumulative (25th - 75th Percentile)</td>
    <td><%= float_to_int(school.admission.act_scores_25th_percentile_cumulative) %> - 
    <%= float_to_int(school.admission.act_scores_75th_percentile_cumulative) %></td>
  </tr>

  <tr>
    <td>ACT - English (25th - 75th Percentile)</td>
    <td><%= float_to_int(school.admission.act_scores_25th_percentile_english) %> - 
    <%= float_to_int(school.admission.act_scores_75th_percentile_english) %></td>
  </tr>

  <tr>
    <td>ACT - Math (25th - 75th Percentile)</td>
    <td><%= float_to_int(school.admission.act_scores_25th_percentile_math) %> - 
    <%= float_to_int(school.admission.act_scores_75th_percentile_math) %></td>
  </tr>

  <tr>
    <td>ACT - Writing (25th - 75th Percentile)</td>
    <td><%= float_to_int(school.admission.act_scores_25th_percentile_writing) %> - 
    <%= float_to_int(school.admission.act_scores_75th_percentile_writing) %></td>
  </tr>

</table>